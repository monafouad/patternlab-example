<div class="row-fluid tc_row__margin">
	<div class="{{panel.class}}">
		<div class="tc_toggle">
		{% include "molecules-toggle" %}
		</div>
		<div class="row-fluid tc_row__margin">
			<h2 class="tc_h2">{{ panel.title }}&nbsp;
			<span class="tc_padding__left--30">{% include "atoms-link-button-with-icon" %}</span>
			</h2>	
		</div>
		<div class="span9">
			<div class="row-fluid tc_row__margin">
				<div class="tc_panel__padding--intro">
					<span class="{{panel.subtitleClass}}">{{ panel.subtitle }}	
					</span>
				</div>
				<div class="row-fluid tc_row__margin">
					{% include "atoms-hr" %}
				</div>
			<div class="row-fluid tc_row__margin">
				{% for item in panelBlock %}
					<div class="{{item.blockClass}}">
						<div class="{{item.col1}}"><i class="{{ item.icon}}"></i></div>
						<div class="{{item.col2}}">{{ item.content }}</div>
					</div>
				{% endfor %}
			</div>
		</div>

		<div class="{{panel.imgContainer}}">
			<div class="row-fluid">
				<div class="pull-right">
					<div class="span6 tc_panel__imgButton--box">
						<a class="{{panel.googleIconClass}}" href="#">
						<img src="{{panel.googleIcon}}" width="80px"/></a>	
						<a class="{{panel.appIconClass}}" href="#">
						<img src="{{panel.appIcon}}" width="80px"/></a>
					</div>
					<div class="span6 tc_panel__img">
						<img src="{{panel.fixedInmage}}" class="{{panel.fixedImageClass}}" />
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

